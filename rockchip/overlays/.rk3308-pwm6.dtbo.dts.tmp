# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm6.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm6.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable PWM6";
  compatible = "radxa,rock-s0";
  category = "misc";
  exclusive = "GPIO0_C2", "uart3";
  description = "Enable PWM6.
On ROCK S0 this is pin 33.";
 };
};

&pwm6 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&pwm6_pin>;
};

&uart3 {
 status = "disabled";
};
