# 1 "arch/arm64/boot/dts/rockchip/overlays/radxa-cm3-io-pcie2x1.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/radxa-cm3-io-pcie2x1.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/radxa-cm3-io-pcie2x1.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/radxa-cm3-io-pcie2x1.dts" 2

/ {
 metadata {
  title = "Enable PCIe";
  compatible = "unknown";
  category = "misc";
  exclusive = "pcie2x1";
  description = "Enable PCIe.
When PCIe is enabled, SATA2 cannot be enabled on the same port.";
 };
};

&{/} {
 vcc-ch482d2-regulator {
  compatible = "regulator-fixed";
  enable-active-low;
  gpio = <&gpio3 22 1>;
  pinctrl-names = "default";
  pinctrl-0 = <&ch482d_en2>;
  regulator-name = "vcc_ch482d2";
  regulator-always-on;
  regulator-boot-on;
  status = "okay";
 };
};

&pcie2x1 {
 status = "okay";
 reset-gpios = <&gpio1 0x0a 0x00>;
 vpcie3v3-supply = <&vcc3v3_sys>;
 pinctrl-0 = <&pcie20m2_pins>;
};

&sata2 {
 status = "disabled";
};

&pinctrl {
 ch482d {
  ch482d_en2: ch482d-en2 {
   rockchip,pins = <3 22 0 &pcfg_pull_none>;
  };
 };
};
