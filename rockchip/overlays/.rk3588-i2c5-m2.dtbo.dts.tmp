# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-i2c5-m2.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-i2c5-m2.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable I2C5-M2";
  compatible = "radxa,cm5-io";
  category = "misc";
  exclusive = "GPIO4_A7", "GPIO4_A6";
  description = "Enable I2C5-M2.
On Radxa CM5 IO this is SDA pin 7 & SCL pin 11.";
 };
};

&i2c5 {
 status = "okay";
 pinctrl-0 = <&i2c5m2_xfer>;
};
