# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-uart2-m0.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-uart2-m0.dts"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-disable-fiq-debugger.dts" 1
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-disable-fiq-debugger.dts" 1
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Disable FIQ Debugger";
  compatible = "rockchip,rk3399";
  category = "misc";
  exclusive = "fiq_debugger";
  description = "Disable FIQ Debugger.";
 };
};

&fiq_debugger {
 status = "okay";
 rockchip,serial-id = <0xffffffff>;
};
# 2 "arch/arm64/boot/dts/rockchip/overlays/rk3568-disable-fiq-debugger.dts" 2

/ {
 metadata {
  compatible = "rockchip,rk3566", "rockchip,rk3568";
 };
};

&fiq_debugger {
 status = "disabled";
};
# 2 "arch/arm64/boot/dts/rockchip/overlays/rk3568-uart2-m0.dts" 2

/ {
 metadata {
  title = "Enable UART2-M0";
  compatible = "radxa,rock-3a", "radxa,rock-3b", "radxa,rock-3c", "radxa,cm3-io", "radxa,cm3-rpi-cm4-io", "radxa,cm3s-io", "radxa,radxa-cm3-sodimm-io", "radxa,e23", "radxa,zero3", "radxa,cm3i-io", "radxa,cm3j-rpi-cm4-io";
  category = "misc";
  exclusive = "GPIO0_D1", "GPIO0_D0", "fiq_debugger";
  description = "Enable UART2-M0.";
 };
};

&uart2 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&uart2m0_xfer>;
};
