# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm2.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm2.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm2.dts" 2

/ {
 metadata {
  title = "Enable PWM2";
  compatible = "radxa,rockpis", "radxa,rock-s0";
  category = "misc";
  exclusive = "GPIO0_B7";
  description = "Enable PWM2.
On Radxa ROCK PI S this is pin 11.
On ROCK S0 this is pin 27.";
 };
};

&pwm2 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&pwm2_pin>;
};

&pwm2_pin {
 rockchip,pins =
  <0 15 1 &pcfg_pull_up_12ma>;
};

&i2c3 {
 status = "disabled";
};
