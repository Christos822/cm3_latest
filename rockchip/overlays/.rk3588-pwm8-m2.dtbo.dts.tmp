# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-pwm8-m2.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-pwm8-m2.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable PWM8-M2";
  compatible = "radxa,cm5-rpi-cm4-io";
  category = "misc";
  exclusive = "GPIO3_D0";
  description = "Enable PWM8-M2.
On Radxa CM5 RPI CM4 IO this is pin 37.";
 };
};

&pwm8 {
 status = "okay";
 pinctrl-0 = <&pwm8m2_pins>;
};
