# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-i2c6.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-i2c6.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable I2C6";
  compatible = "rockchip,rk3399";
  category = "misc";
  exclusive = "GPIO2_B2", "GPIO2_B1";
  description = "Enable I2C6 on pin 29 & 31.";
 };
};

&i2c6 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&i2c6_xfer>;
};
