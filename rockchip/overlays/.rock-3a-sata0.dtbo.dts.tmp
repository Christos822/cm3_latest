# 1 "arch/arm64/boot/dts/rockchip/overlays/rock-3a-sata0.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rock-3a-sata0.dts"

/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable SATA0";
  compatible = "unknown";
  category = "misc";
  description = "Enable SATA0.
When SATA0 is enabled, USB3.0 cannot be enabled on the same port.";
 };
};

&usbdrd_dwc3 {
 status = "disabled";
};

&usbdrd30 {
 status = "disabled";
};

&sata0 {
 status = "okay";
};
