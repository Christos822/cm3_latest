# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi2-enc28j60.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi2-enc28j60.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi2-enc28j60.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi2-enc28j60.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/interrupt-controller/irq.h" 1
# 7 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi2-enc28j60.dts" 2

/ {
 metadata {
  title = "Enable ENC28J60 on SPI2";
  compatible = "unknown";
  category = "misc";
  exclusive = "GPIO2_B3", "GPIO2_B2", "GPIO2_B1", "GPIO2_B4", "GPIO4_A7";
  description = "Enable Microchip ENC28J60 SPI Ethernet controller on SPI2.
INT=40";
 };
};

&spi2 {
 status = "okay";
 #address-cells = <1>;
 #size-cells = <0>;
 pinctrl-names = "default";
 pinctrl-0 = <&spi2_clk &spi2_cs0 &spi2_rx &spi2_tx>;

 ethernet: enc28j60@0 {
  compatible = "microchip,enc28j60";
  reg = <0>;
  spi-max-frequency = <20000000>;

  pinctrl-names = "default";
  pinctrl-0 = <&enc28j60_int_pins>;

  interrupt-parent = <&gpio4>;
  interrupts = <7 2>;
 };
};

&pinctrl {
 enc28j60 {
  enc28j60_int_pins: enc28j60-int-pins {
   rockchip,pins = <4 7 0 &pcfg_pull_up>;
  };
 };
};
