# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-spi1-m1-cs1-mcp2515-8mhz.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-spi1-m1-cs1-mcp2515-8mhz.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3588-spi1-m1-cs1-mcp2515-8mhz.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/rk3588-spi1-m1-cs1-mcp2515-8mhz.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/interrupt-controller/irq.h" 1
# 7 "arch/arm64/boot/dts/rockchip/overlays/rk3588-spi1-m1-cs1-mcp2515-8mhz.dts" 2

/ {
 metadata {
  title = "Enable MCP2515 with 8MHz external clock on SPI1-M1 over CS1";
  compatible = "radxa,rock-5b", "radxa,rock-5b-plus", "radxa,rock-5t";
  category = "misc";
  exclusive = "GPIO3_C3", "GPIO3_C1", "GPIO3_B7", "GPIO3_C0", "GPIO1_B2";
  description = "Provide support for Microchip MCP2515 SPI CAN controller.
Assumes 8MHz external clock.
Uses Pin 19 (GPIOI1_B2) for INT.";
 };
};

&spi1 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&spi1m1_pins &spi1m1_cs0 &spi1m1_cs1>;
 #address-cells = <1>;
 #size-cells = <0>;
 max-freq = <1000000>;

 can0: mcp2515@1 {
  status = "okay";
  compatible = "microchip,mcp2515";
  reg = <1>;
  spi-max-frequency = <1000000>;

  pinctrl-names = "default";
  pinctrl-0 = <&mcp2515_int_pins>;

  interrupt-parent = <&gpio1>;
  interrupts = <10 2>;

  clocks = <&can0_osc>;
  vdd-supply = <&vcc_3v3_s3>;
  xceiver-supply = <&vcc_3v3_s3>;
 };
};

&pinctrl {
 mcp2515 {
  mcp2515_int_pins: mcp2515-int-pins {
   rockchip,pins = <1 10 0 &pcfg_pull_none>;
  };
 };
};

&{/} {
 can0_osc: can0-osc {
  compatible = "fixed-clock";
  #clock-cells = <0>;
  clock-frequency = <8000000>;
 };
};
