# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-rga-400.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-rga-400.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/clock/rk3399-cru.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3399-rga-400.dts" 2

/ {
 metadata {
  title = "Set RGA frequency to 400 MHz";
  compatible = "unknown";
  category = "misc";
  description = "Set RGA frequency to 400 MHz.
This is usually not needed.
You can check current clock speed with:
sudo grep rga /sys/kernel/debug/clk/clk_summary";
 };
};

&rga {
 assigned-clocks = <&cru 220>, <&cru 109>;
 assigned-clock-rates = <400000000>, <400000000>;
};
