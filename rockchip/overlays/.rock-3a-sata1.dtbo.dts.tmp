# 1 "arch/arm64/boot/dts/rockchip/overlays/rock-3a-sata1.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rock-3a-sata1.dts"

/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable SATA1";
  compatible = "unknown";
  category = "misc";
  description = "Enable SATA1.
When SATA1 is enabled, USB3.0 cannot be enabled on the same port.";
 };
};

&usbhost_dwc3 {
 status = "disabled";
};

&usbhost30 {
 status = "disabled";
};

&sata1 {
 status = "okay";
};
