# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-fiq-debugger-uart2m0.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-fiq-debugger-uart2m0.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable FIQ Debugger on UART2-M0";
  compatible = "unknown";
  category = "misc";
  exclusive = "GPIO0_D1", "GPIO0_D0";
  description = "Enable FIQ Debugger on UART2-M0.";
 };
};

&fiq_debugger {
 status = "okay";
 rockchip,serial-id = <2>;
 pinctrl-names = "default";
 pinctrl-0 = <&uart2m0_xfer>;
};

&uart2 {
 status = "disabled";
};
