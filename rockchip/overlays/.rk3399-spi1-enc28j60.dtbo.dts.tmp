# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi1-enc28j60.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi1-enc28j60.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi1-enc28j60.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi1-enc28j60.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/interrupt-controller/irq.h" 1
# 7 "arch/arm64/boot/dts/rockchip/overlays/rk3399-spi1-enc28j60.dts" 2

/ {
 metadata {
  title = "Enable ENC28J60 on SPI1";
  compatible = "radxa,rock-4c-plus";
  category = "misc";
  exclusive = "GPIO1_A7", "GPIO1_B0", "GPIO1_B1", "GPIO1_B2", "GPIO4_D5";
  description = "Provide support for Microchip ENC28J60 SPI Ethernet controller on GPIO pins.
Pin definition:
MOSI=19 MISO=21 SCK=23 CS=24 INT=22";
 };
};

&spi1 {
 #address-cells = <1>;
 #size-cells = <0>;
 status = "okay";

 ethernet: enc28j60@0 {
  compatible = "microchip,enc28j60";
  reg = <0>;
  spi-max-frequency = <20000000>;
  interrupt-parent = <&gpio4>;
  interrupts = <29 2>;
  pinctrl-names = "default";
  pinctrl-0 = <&enc28j60_int_pins>;
 };
};

&pinctrl {
 enc28j60 {
  enc28j60_int_pins: enc28j60-int-pins {
   rockchip,pins = <4 29 0 &pcfg_pull_up>;
  };
 };
};
