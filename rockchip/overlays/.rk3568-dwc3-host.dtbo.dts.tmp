# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-dwc3-host.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-dwc3-host.dts"

/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Set OTG port to Host mode";
  compatible = "rockchip,rk3566", "rockchip,rk3568";
  category = "misc";
  exclusive = "usbdrd_dwc3-dr_mode";
  description = "Set OTG port to Host mode.
Use this when you want to connect USB devices.";
 };
};

&usbdrd_dwc3 {
 status = "okay";
 dr_mode = "host";
};
