# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-w1-gpio3-a5.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-w1-gpio3-a5.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3568-w1-gpio3-a5.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/rk3568-w1-gpio3-a5.dts" 2

/ {
 metadata {
  title = "Enable 1-Wire on GPIO3_A5";
  compatible = "radxa,rock-3a", "radxa,rock-3b", "radxa,rock-3c";
  category = "misc";
  exclusive = "GPIO3_A5";
  description = "Enable 1-Wire on GPIO3_A5.
On Radxa ROCK 3A this is pin 40.
On Radxa ROCK 3B this is pin 40.
On Radxa ROCK 3C this is pin 40.";
 };
};

&{/} {
 w1: onewire {
  compatible = "w1-gpio";
  gpios = <&gpio3 5 0>;
  status = "okay";
 };
};
