# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-w1-gpio4-b1.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-w1-gpio4-b1.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3588-w1-gpio4-b1.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/rk3588-w1-gpio4-b1.dts" 2

/ {
 metadata {
  title = "Enable 1-Wire on GPIO4_B1";
  compatible = "radxa,rock-5a", "radxa,rock-5c", "radxa,rock-5d";
  category = "misc";
  exclusive = "GPIO4_B1";
  description = "Enable 1-Wire on GPIO4_B1.
On Radxa ROCK 5A this is pin 40.
On Radxa ROCK 5C this is pin 40.
On Radxa ROCK 5D this is pin 40.
";
 };
};

&{/} {
 w1: onewire {
  compatible = "w1-gpio";
  gpios = <&gpio4 9 0>;
  status = "okay";
 };
};
