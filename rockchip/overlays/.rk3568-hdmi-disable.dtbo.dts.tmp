# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-hdmi-disable.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-hdmi-disable.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Disable HDMI";
  compatible = "rockchip,rk3568", "rockchip,rk3566";
  category = "misc";
  exclusive = "hdmi", "hdmi_sound";
  description = "Disable HDMI.";
 };
};

&hdmi {
 status = "disabled";
};

&hdmi_sound {
 status = "disabled";
};
