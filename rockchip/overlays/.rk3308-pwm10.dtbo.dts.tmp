# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm10.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm10.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable PWM10";
  compatible = "radxa,rock-s0";
  category = "misc";
  exclusive = "GPIO2_B4";
  description = "Enable PWM10.
On ROCK S0 this is pin 32.";
 };
};

&pwm10 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&pwm10_pin>;
};
