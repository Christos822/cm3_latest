# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-i2c2.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-i2c2.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable I2C2";
  compatible = "radxa,rock-s0";
  category = "misc";
  exclusive = "GPIO2_A2", "GPIO2_A3", "pwm2";
  description = "Enable I2C2.
On ROCK S0 this is SDA pin 37 & SCL pin 13.";
    };
};

&i2c2 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&i2c2_xfer>;
};

&spi0 {
 status = "disabled";
};
