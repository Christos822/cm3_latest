# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-pwm3-m2.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-pwm3-m2.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable PWM3-M2";
  compatible = "radxa,cm5-rpi-cm4-io";
  category = "misc";
  exclusive = "GPIO1_C2";
  description = "Enable PWM3-M2.
On Radxa CM5 RPI CM4 IO this is pin 11.";
 };
};

&pwm3 {
 status = "okay";
 pinctrl-0 = <&pwm3m2_pins>;
};
