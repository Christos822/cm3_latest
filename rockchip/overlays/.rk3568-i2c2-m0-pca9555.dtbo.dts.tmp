# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-i2c2-m0-pca9555.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-i2c2-m0-pca9555.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3568-i2c2-m0-pca9555.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/rk3568-i2c2-m0-pca9555.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/interrupt-controller/irq.h" 1
# 7 "arch/arm64/boot/dts/rockchip/overlays/rk3568-i2c2-m0-pca9555.dts" 2

/{
 metadata {
  title = "Enable NXP PCA9555 I2C GPIO multiplexer on I2C2-M0";
  compatible = "radxa,rock-3a", "radxa,rock-3b", "radxa,cm3-io", "radxa,cm3s-io", "radxa,radxa-cm3-sodimm-io", "radxa,e23";
  category = "misc";
  exclusive = "GPIO0_B5", "GPIO0_B6";
  description = "Enable NXP PCA9555 I2C GPIO multiplexer on I2C2-M0.";
 };
};

&i2c2 {
 status = "okay";
 #address-cells = <1>;
 #size-cells = <0>;
 pinctrl-names = "default";
 pinctrl-0 = <&i2c2m0_xfer>;

 pca: pca@20 {
  compatible = "nxp,pca9555";
  reg = <0x20>;
  gpio-controller;
  #gpio-cells = <2>;
  interrupt-controller;
  #interrupt-cells = <2>;
  interrupt-parent = <&gpio0>;
  interrupts = <23 4>;
  pinctrl-names = "default";
  pinctrl-0 = <&pca_int>;
  status = "okay";
 };
};

&pinctrl {
 pca {
  pca_int: pca-int {
   rockchip,pins = <0 23 0 &pcfg_pull_none>;
  };
 };
};
