# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-uart0.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable UART0";
  compatible = "unknown";
  category = "misc";
  description = "Enable UART0.";
