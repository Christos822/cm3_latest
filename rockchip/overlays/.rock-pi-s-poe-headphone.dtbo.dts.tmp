# 1 "arch/arm64/boot/dts/rockchip/overlays/rock-pi-s-poe-headphone.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rock-pi-s-poe-headphone.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rock-pi-s-poe-headphone.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/rock-pi-s-poe-headphone.dts" 2

/ {
 metadata {
  title = "Enable PoE HAT headphone";
  compatible = "radxa,rockpis";
  category = "misc";
  exclusive = "GPIO0_B7";
  description = "Enable PoE HAT headphone.";
 };
};

&{/} {
 hp_en: hp-en {
  compatible = "regulator-fixed";
  regulator-name = "hp_en";
  regulator-always-on;
  regulator-boot-on;
  gpio = <&gpio0 15 0>;
  enable-active-high;
  pinctrl-names = "default";
  pinctrl-0 = <&headphone_en>;
 };
};

&pinctrl {
 poe_hat {
  headphone_en: headphone-en {
   rockchip,pins = <0 15 0 &pcfg_pull_up>;
  };
 };
};
