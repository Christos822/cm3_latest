# 1 "arch/arm64/boot/dts/rockchip/overlays/rockchip-gpu-disable.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rockchip-gpu-disable.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Disable GPU";
  compatible = "rockchip,rk3308", "rockchip,rk3328", "rockchip,rk3399", "rockchip,rk3566", "rockchip,rk3568", "rockchip,rk3588";
  category = "misc";
  exclusive = "gpu";
  description = "Disable GPU.";
 };
};

&gpu {
 status = "disabled";
};
