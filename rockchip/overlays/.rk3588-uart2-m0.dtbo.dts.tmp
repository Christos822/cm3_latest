# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-uart2-m0.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-uart2-m0.dts"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-disable-fiq-debugger.dts" 1
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-disable-fiq-debugger.dts" 1
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-disable-fiq-debugger.dts" 1
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Disable FIQ Debugger";
  compatible = "rockchip,rk3399";
  category = "misc";
  exclusive = "fiq_debugger";
  description = "Disable FIQ Debugger.";
 };
};

&fiq_debugger {
 status = "okay";
 rockchip,serial-id = <0xffffffff>;
};
# 2 "arch/arm64/boot/dts/rockchip/overlays/rk3568-disable-fiq-debugger.dts" 2

/ {
 metadata {
  compatible = "rockchip,rk3566", "rockchip,rk3568";
 };
};

&fiq_debugger {
 status = "disabled";
};
# 2 "arch/arm64/boot/dts/rockchip/overlays/rk3588-disable-fiq-debugger.dts" 2

/ {
 metadata {
  compatible = "rockchip,rk3588";
 };
};
# 2 "arch/arm64/boot/dts/rockchip/overlays/rk3588-uart2-m0.dts" 2

/ {
 metadata {
  title = "Enable UART2-M0";
  compatible = "radxa,rock-5a", "radxa,rock-5b", "radxa,rock-5b-plus", "radxa,rock-5c", "radxa,rock-5d", "radxa,rock-5t", "radxa,nx5-io", "radxa,cm5-io", "radxa,cm5-rpi-cm4-io";
  category = "misc";
  exclusive = "GPIO0_B5", "GPIO0_B6", "fiq_debugger";
  description = "Enable UART2-M0.
On Radxa ROCK 5A this is TX pin 8 & RX pin 10.
On Radxa ROCK 5B this is TX pin 8 & RX pin 10.
On Radxa ROCK 5B+ this is TX pin 8 & RX pin 10.
On Radxa ROCK 5C this is TX pin 8 & RX pin 10.
On Radxa ROCK 5D this is TX pin 8 & RX pin 10.
On Radxa ROCK 5T this is TX pin 8 & RX pin 10.
On Radxa NX5 IO this is TX pin 8 & RX pin 10.
On Radxa CM5 IO this is TX pin 8 & RX pin 10.
On Radxa CM5 RPI CM4 IO this is TX pin 8 & RX pin 10.
";
 };
};

&uart2 {
 status = "okay";
 pinctrl-0 = <&uart2m0_xfer>;
};
