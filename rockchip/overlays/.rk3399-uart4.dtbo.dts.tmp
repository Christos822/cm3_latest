# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-uart4.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-uart4.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable UART4";
  compatible = "rockchip,rk3399";
  category = "misc";
  exclusive = "GPIO1_B0", "GPIO1_A7";
  description = "Enable UART4.";
 };
};

&uart4 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&uart4_xfer>;
};
