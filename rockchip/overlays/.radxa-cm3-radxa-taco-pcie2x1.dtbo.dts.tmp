# 1 "arch/arm64/boot/dts/rockchip/overlays/radxa-cm3-radxa-taco-pcie2x1.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/radxa-cm3-radxa-taco-pcie2x1.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/radxa-cm3-radxa-taco-pcie2x1.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/radxa-cm3-radxa-taco-pcie2x1.dts" 2

/ {
 metadata {
  title = "Enable PCIe on Radxa Taco";
  compatible = "radxa,cm3-io", "radxa,cm3-rpi-cm4-io";
  category = "misc";
  exclusive = "pcie2x1";
  description = "Enable PCIe on Radxa Taco.
When PCIe is enabled, SATA2 cannot be enabled on the same port.";
 };
};

&{/} {
 pcie2x1-en-regulator {
  compatible = "regulator-fixed";
  enable-active-high;
  gpio = <&gpio4 6 0>;
  pinctrl-names = "default";
  pinctrl-0 = <&pcie2x1_en>;
  regulator-name = "pcie2x1_en";
  regulator-always-on;
  regulator-boot-on;
  status = "okay";
 };
};

&pcie2x1 {
 status = "okay";
 reset-gpios = <&gpio1 10 0>;
 vpcie3v3-supply = <&vcc3v3_sys>;
 pinctrl-0 = <&pcie20m2_pins>;
};

&sata2 {
 status = "disabled";
};

&pinctrl {
 pcie2x1_en_gpio{
  pcie2x1_en: pcie2x1-en{
   rockchip,pins = <3 22 0 &pcfg_pull_none>;
  };
 };
};
