# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-serdes2-pcie.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-serdes2-pcie.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3568-serdes2-pcie.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/rk3568-serdes2-pcie.dts" 2

/ {
 metadata {
  title = "Enable PCIe 2.0 x1 on SERDES2";
  compatible = "radxa,cm3s-io", "radxa,radxa-cm3-sodimm-io";
  category = "misc";
  exclusive = "vcc_ch482d", "sata2", "pcie2x1";
  description = "Enable PCIe 2.0 x1 on SERDES2.
This will disable SATA2.";
 };
};

&vcc_ch482d {
 enable-active-low;
 gpio = <&gpio0 17 1>;
};

&sata2 {
 status = "disabled";
};

&pcie2x1 {
 status = "okay";
};
