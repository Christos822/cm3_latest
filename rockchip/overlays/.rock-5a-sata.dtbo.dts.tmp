# 1 "arch/arm64/boot/dts/rockchip/overlays/rock-5a-sata.dts"
/dts-v1/;
/plugin/;

/ {

 metadata {
  title = "Enable SATA0";
  compatible = "radxa,rock-5a";
  category = "misc";
  exclusive = "combphy0_ps";
  description = "Enable SATA0.
When SATA0 is enabled, PCIe cannot be enabled on the same port.";
 };
};

&pcie2x1l2 {
 status = "disabled";
};

&sata0 {
 status = "okay";
};
