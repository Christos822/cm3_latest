# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-uart2.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3399-uart2.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable UART2";
  compatible = "rockchip,rk3399";
  category = "misc";
  exclusive = "GPIO4_C4", "GPIO4_C3";
  description = "Enable UART2.";
 };
};

&uart2 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&uart2c_xfer>;
};
