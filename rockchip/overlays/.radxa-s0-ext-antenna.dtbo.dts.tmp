# 1 "arch/arm64/boot/dts/rockchip/overlays/radxa-s0-ext-antenna.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/radxa-s0-ext-antenna.dts"
/dts-v1/;
/plugin/;
# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 4 "arch/arm64/boot/dts/rockchip/overlays/radxa-s0-ext-antenna.dts" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/radxa-s0-ext-antenna.dts" 2

/ {
 metadata {
  title = "Enable External Antenna";
  compatible = "radxa,rock-s0";
  category = "misc";
  exclusive = "GPIO0_A6", "GPIO0_A5";
  description = "Enable External Antenna.";
 };
};

&{/} {
 ext_antenna: ext-antenna {
  status = "okay";
  compatible = "regulator-fixed";
  enable-active-low;
  gpio = <&gpio0 6 1>;
  regulator-always-on;
  regulator-boot-on;
  pinctrl-0 = <&ant_2>;
  pinctrl-names = "default";
  regulator-name = "ext_antenna";
 };
};

&board_antenna {
 status = "disabled";
};

&pinctrl {
 antenna {
  ant_2: ant-2 {
   rockchip,pins = <0 6 0 &pcfg_pull_down>;
  };
 };
};
