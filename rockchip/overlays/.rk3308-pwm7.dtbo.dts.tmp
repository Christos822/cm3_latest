# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm7.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm7.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable PWM7";
  compatible = "radxa,rock-s0";
  category = "misc";
  exclusive = "GPIO0_C0";
  description = "Enable PWM7.
On ROCK S0 this is pin 26.";
 };
};

&pwm7 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&pwm7_pin>;
};
