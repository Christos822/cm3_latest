# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm3.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm3.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm3.dts" 2

/ {
 metadata {
  title = "Enable PWM3";
  compatible = "radxa,rockpis", "radxa,rock-s0";
  category = "misc";
  exclusive = "GPIO0_C0", "i2c3";
  description = "Enable PWM3.
On Radxa ROCK PI S this is pin 13.
On Radxa ROCK S this is pin 28.";
 };
};

&pwm3 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&pwm3_pin>;
};

&pwm3_pin {
 rockchip,pins =
  <0 16 1 &pcfg_pull_up_12ma>;
};

&i2c3 {
 status = "disabled";
};
