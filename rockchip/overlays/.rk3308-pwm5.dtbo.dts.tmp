# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm5.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3308-pwm5.dts"
/dts-v1/;
/plugin/;

/ {
 metadata {
  title = "Enable PWM5";
  compatible = "radxa,rock-s0";
  category = "misc";
  exclusive = "GPIO0_C1", "uart3";
  description = "Enable PWM5.
On ROCK S0 this is pin 15.";
 };
};

&pwm5 {
 status = "okay";
 pinctrl-names = "default";
 pinctrl-0 = <&pwm5_pin>;
};

&uart3 {
 status = "disabled";
};
