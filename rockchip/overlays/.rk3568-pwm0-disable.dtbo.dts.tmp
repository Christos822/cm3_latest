# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-pwm0-disable.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3568-pwm0-disable.dts"
/dts-v1/;
/plugin/;

# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/rk3568-pwm0-disable.dts" 2

/ {
 metadata {
  title = "Disable FAN0";
  compatible = "radxa,cm3-io";
  category = "misc";
  exclusive = "fan0";
  description = "Disable FAN0.
This will also disable PWM0, so the pin can be used for other functions.";
 };
};

&fan0 {
 status = "disabled";
};

&pwm0 {
 status = "disabled";
};
