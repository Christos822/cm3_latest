# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-spi4-m2-cs0-waveshare35.dts"
# 1 "<built-in>"
# 1 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlays/rk3588-spi4-m2-cs0-waveshare35.dts"

# 1 "arch/arm64/boot/dts/rockchip/overlays/waveshare35-lcd.dtsi" 1



# 1 "./scripts/dtc/include-prefixes/dt-bindings/gpio/gpio.h" 1
# 5 "arch/arm64/boot/dts/rockchip/overlays/waveshare35-lcd.dtsi" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/pinctrl/rockchip.h" 1
# 6 "arch/arm64/boot/dts/rockchip/overlays/waveshare35-lcd.dtsi" 2
# 1 "./scripts/dtc/include-prefixes/dt-bindings/interrupt-controller/irq.h" 1
# 7 "arch/arm64/boot/dts/rockchip/overlays/waveshare35-lcd.dtsi" 2

/dts-v1/;
/plugin/;





&spi4 {
 status = "okay";
 #address-cells = <1>;
 #size-cells = <0>;

 ili9486: ili9486@0 {
  compatible = "ilitek,ili9486";
  reg = <0>;
  spi-max-frequency = <16000000>;
  txbuflen = <32768>;
  rotate = <90>;
  bgr = <0>;
  fps = <30>;
  buswidth = <8>;
  regwidth = <16>;
  debug = <0>;
 };

 ads7846: ads7846@1 {
  compatible = "ti,ads7846";
  status = "okay";
  reg = <1>;
  id = <1>;
  spi-max-frequency = <2000000>;
  ti,x-plate-ohms = /bits/ 16 <60>;
  ti,pressure-max = /bits/ 16 <255>;
  ti,swap-xy = <0>;
  ti,invert-y = <1>;
  vcc-supply = <&vcc5v0_sys>;
 };
};
# 3 "arch/arm64/boot/dts/rockchip/overlays/rk3588-spi4-m2-cs0-waveshare35.dts" 2

/ {
 metadata {
  title = "Enable Waveshare 3.5inch RPi LCD (C) on SPI4";
  compatible = "radxa,rock-5a", "radxa,rock-5c", "radxa,rock-5d";
  category = "misc";
  exclusive = "GPIO1_A2", "GPIO1_A0", "GPIO1_A1", "GPIO1_A3", "GPIO1_B0", "GPIO1_B5", "GPIO4_B3";
  description = "Enable Waveshare 3.5inch RPi LCD (C) on SPI4.
Touch panel is currently unsupported.";
 };
};

&spi4 {
 pinctrl-0 = <&spi4m2_pins>;
 num-cs = <2>;
 cs-gpios = <&gpio1 3 1
   &gpio1 4 1>;
};

&ili9486 {
 reset-gpios = <&gpio1 13 1>;
 dc-gpios = <&gpio1 8 0>;
};

&ads7846 {
 status = "disabled";
 interrupt-parent = <&gpio4>;
 interrupts = <11 2>;
 pendown-gpio = <&gpio4 11 0>;
};
