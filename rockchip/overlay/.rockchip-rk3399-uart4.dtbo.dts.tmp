# 0 "arch/arm64/boot/dts/rockchip/overlay/rockchip-rk3399-uart4.dtso"
# 0 "<built-in>"
# 0 "<command-line>"
# 1 "arch/arm64/boot/dts/rockchip/overlay/rockchip-rk3399-uart4.dtso"
/dts-v1/;
/plugin/;

/ {
 compatible = "rockchip,rk3399";

 fragment@0 {
  target = <&spi1>;
  __overlay__ {
   status = "disabled";
  };
 };

 fragment@1 {
  target = <&uart4>;
  __overlay__ {
   status = "okay";
  };
 };
};
